package thread;

class MyThread2 extends Thread{
	
	@Override
	public void run() {
		int sum= 0;
		for(int i = 0 ; i < 10 ; i++) {
			sum = sum + i;
		}
		String name = Thread.currentThread().getName();
		System.out.println(name + " : " +sum);
	}
}


public class Ex02_ThreadClass {

	
	public static void main(String[] args) {
		
	}
}
